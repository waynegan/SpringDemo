<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"   "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>


	<bean id="Triangle" class="gq.bithub.wayne.springDemo.Triangle" autowire="byName">
		
	</bean>
	<bean id="pointA" class="gq.bithub.wayne.springDemo.Point">
		<property  name="X" value="0"  />
		<property     name="y" value="0" />
	</bean>
	<bean id="pointB" class="gq.bithub.wayne.springDemo.Point">
		<property  name="X" value="-20"  />
		<property     name="y" value="0" />
	</bean>
	<bean id="pointC" class="gq.bithub.wayne.springDemo.Point">
		<property     name="x" value="20" />
		<property   name="y" value="0"  />
	</bean>
	

</beans>
